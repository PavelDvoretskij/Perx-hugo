{
  "name": "video-hugo-sass-bootstrap",
  "version": "0.1.0",
  "description": "Using sass with hugo and bootstrap",
  "main": " ",
  "scripts": {
    "dev": "exec-bin node_modules/.bin/hugo/hugo server --gc --disableFastRender",
    "dev:drafts": "exec-bin node_modules/.bin/hugo/hugo server --gc --disableFastRender --buildFuture --buildDrafts",
    "build": "exec-bin node_modules/.bin/hugo/hugo --gc --cleanDestinationDir --minify",
    "start:dev": "exec-bin node_modules/.bin/hugo/hugo server --gc --disableFastRender  --cleanDestinationDir",
    "start:prod": "exec-bin node_modules/.bin/hugo/hugo server --gc --disableFastRender  --cleanDestinationDir --minify --environment production",
    "npm-check": "npm-check -u",
    "postinstall": "hugo-installer --version otherDependencies.hugo --extended --destination node_modules/.bin/hugo"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/future-wd/video-hugo-sass-bootstrap"
  },
  "keywords": [],
  "author": "Future Web Design",
  "license": "ISC",
  "devDependencies": {
    "exec-bin": "^1.0.0",
    "hugo-installer": "^4.0.1",
    "npm-check": "^6.0.1"
  },
  "otherDependencies": {
    "hugo": "0.124.1"
  },
  "dependencies": {
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0"
  }
}
